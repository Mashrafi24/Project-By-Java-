// Class
class Student {
    String name;
    int id;

    void displayInfo() {
        System.out.println("Name: " + name);
        System.out.println("ID: " + id);
    }
}

// Main class
public class Main {
    public static void main(String[] args) {

        // Object creation
        Student s1 = new Student();

        // Assign values
        s1.name = "Mash";
        s1.id = 101;

        // Call method
        s1.displayInfo();
    }
}
